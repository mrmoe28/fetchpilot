    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2"
  }
}
