{
  "name": "fetchpilot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "tsx worker/browser-worker.ts",
    "postinstall": "playwright install chromium --with-deps || playwright install chromium || echo 'Playwright browser installation skipped'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "verify:scrape": "export $(grep -v '^#' .env.local | xargs) && tsx scripts/run-scrape.ts"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.3",
    "@neondatabase/serverless": "^0.10.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.441.0",
    "nanoid": "^5.0.9",
    "next": "^15.5.3",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "playwright": "^1.56.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
